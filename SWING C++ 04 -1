#include <iostream>

using namespace std;

class Box
{
private:
	char f;
	int w=0;
	int h=0;

public:

	Box(int w, int h)
	{
		w = 3;
		h = 4;
		f = '*';
	}

	void setFil(char f)
	{
		if (f)
			cin >> f;
	}

	void setSize(int w, int h)
	{
		cin >> w;
		cin >> h;
	}

	void draw()
	{
		for (int i = 0; i < h; i++)
		{
			for (int j = 0; j < w; j++)
			{
				cout << f;
			}
			cout << endl;
		}
	}
};


int main(void)
{
	Box box1(3, 4);
	box1.setFil('*');
	box1.setSize(3,4);
	box1.draw();

	Box box2(6, 10);
	box2.setFil('^');
	box2.setSize(6,10);
	box2.draw();

	return 0;
}
